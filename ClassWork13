queue = []
def enqueue():

    n = input("Enter element for the queue: ")
    queue.append(n)

def dequeue():
    queue.remove(queue[0])

def display():
    print(queue)

while 1:
    print("1. Enqueue")
    print("2. Dequeue")
    print("3. Display")

    option = input("Enter your choice: ")

    if option == "1":
        enqueue()

    elif option == "2":
        dequeue()

    elif option == "3":
        display()
