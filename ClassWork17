import pickle

d = {"stu1": {"Name": "John", "Age": "21", "Id": 28},
    "stu2": {"Name": "Bob", "Age": "43", "Id": 29},
    "stu3": {"Name": "Dean", "Age": "32", "Id": 30}
     }

with open("students.dat", "rb") as file2:
    mydictionary = pickle.load(file2)
file2.close()

print(mydictionary)

i=1
for x in mydictionary:
    var = "stu"+str(i)
    print(mydictionary[var]["Name"])
    i+=1
